 <section class="border-top">
	<div class="container">
		<div class="page-title mrgb6x mrgt6x clearfix">
      <h4 class="page-name"><%= current_user.email %></h4>
      <div class="tag-bar"> <a href="#"><span>Recent Bookings</span></a> </div>
			<ul class="breadcrumb">
				<li><a href="#">Pages</a></li>
				<li class="active"><a href="#">Bookings booked</a></li>
			</ul>
		</div>
	</div>
</section>

<% @bookings.each do |b| %>  

  <div class="container">
    <div class="row">
      <div class="col-md-3">
        <%= link_to cl_image_tag(b.desk.image_url(:mini)), Space.find(b.space_id) %>
      </div>

      <div class="col-md-3">
          <div class="alert alert-info" role="alert"> <i class="fa fa-info-circle"></i> 
            
            <a href="#" class="alert-link"> <%= Space.find(b.space_id).name %>:<br></a> <%= b.start_time.strftime("%b/%d/%Y")  %> at <%= b.start_time.strftime("%I:%M%p")  %> for <%= b.length %> hours</div>
        </div>  
     
     </div> 
  </div>
 <br>
       <% end %>

				


